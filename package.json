{
  "name": "chartcat",
  "version": "1.0.0",
  "description": "ChartCat application",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -- ts-node server.ts",
    "build": "next build src",
    "start": "NODE_ENV=production ts-node server.ts",
    "test": "dotenv -- jest",
    "debug": "dotenv -- ts-node --inspect server.ts"
  },
  "engines": {
    "node": ">=10 <11"
  },
  "dependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/jsonwebtoken": "7.2.7",
    "@types/next": "6.0.4",
    "@types/node": "10.5.2",
    "@types/uuid": "3.4.3",
    "@zeit/next-sass": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "apollo-link-http": "1.5.4",
    "apollo-server-express": "2.1.0",
    "dotenv-cli": "1.4.0",
    "express": "4.16.4",
    "graphql-import": "0.7.1",
    "next": "7.0.1",
    "next-apollo": "2.0.0",
    "node-sass": "4.9.2",
    "pg": "7.5.0",
    "react": "16.4.1",
    "react-apollo": "2.1.9",
    "react-dom": "16.4.1",
    "ts-node": "7.0.0",
    "typescript": "2.9.2"
  },
  "devDependencies": {
    "@types/jest": "23.1.1",
    "@types/supertest": "2.0.4",
    "jest": "23.1.0",
    "supertest": "3.1.0",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
